<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="FormsGallery.ImageCellDemoPage"
             Title="ImageCell">
    <ContentPage.Content>
        <TableView Intent="Form">
            <TableSection Title="Table Section">
                <ImageCell AutomationId="ImageCell" Text="This is a ImageCell" ImageSource="https://www.xamarin.com/content/images/pages/branding/assets/xamagon.png"/>
            </TableSection>
        </TableView>
    </ContentPage.Content>
</ContentPage>